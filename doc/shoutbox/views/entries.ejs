<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% include menu %>
    <h1><%= title %></h1>
    <% entries.forEach(function(entry) { %>
      <p class='<%= message.type %>'>
        <%= message.string %>
      </p>
      <p> Post by <% entry.username %> </p>
      <% }) %>
  </body>
</html>